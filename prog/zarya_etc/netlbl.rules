
#!/bin/bash

service netlabel start
netlabelctl map del default
netlabelctl cipsov4 add pass doi:10 tags:1
netlabelctl map add default address:0.0.0.0/0 protocol:unlbl
netlabelctl unlbl add default address:0.0.0.0/0 label:system_u:object_r:netlabel_peer_t:s0

netlabelctl map add default address:192.168.10.45/32 protocol:cipsov4,10
netlabelctl map add default address:192.168.10.46/32 protocol:cipsov4,10
netlabelctl map add default address:192.168.10.47/32 protocol:cipsov4,10
